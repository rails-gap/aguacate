<% if projects.any? %>
  <div class="row">
    <% projects.each do |project| %>
      <div class="col-sm-12">
        <%= link_to project_path(project) do %>
          <%= project.name %>
        <% end %>
      </div>
    <% end %>
  </div>
  <%= will_paginate projects, renderer: BootstrapPagination::Rails %>
<% else %>
  No Projects found.
<% end %>
